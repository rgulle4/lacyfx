      <Label fx:id="enp_score" layoutX="237.0" layoutY="312.0" />
      <ButtonBar layoutX="481.0" layoutY="440.0" prefHeight="40.0" prefWidth="247.0">
        <buttons>
          <Button mnemonicParsing="false" onAction="#showReport" text="View Reports" />
        </buttons>
      </ButtonBar>
      <Label fx:id="computation_Flag" layoutX="437.0" layoutY="453.0" text="Calcualtion are complete!" />
        <Button layoutX="340.0" layoutY="449.0" mnemonicParsing="false" onAction="#computeButton" text="Compute" />
