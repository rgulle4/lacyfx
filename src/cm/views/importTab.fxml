<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cm.controllers.ImportTabController">
    <children>
        <ButtonBar layoutX="414.0" layoutY="350.0" prefHeight="40.0" prefWidth="200.0">
            <buttons>
                <Button mnemonicParsing="false" onAction="#nextButton" text="Next" />
                <Button mnemonicParsing="false" text="Close" />
            </buttons>
        </ButtonBar>
      <VBox layoutX="182.0" layoutY="76.0" prefHeight="204.0" prefWidth="240.0" spacing="10.0">
         <children>
            <Label text="Design type:" />
            <ComboBox fx:id="Designtype" onAction="#SelectDesignType" prefWidth="150.0" />
            <Label fx:id="label1" text="Pavement type:" />
            <ComboBox fx:id="Pavementtype" prefWidth="150.0" />
            <Label text="Number of layers to be analyized:" />
            <ComboBox fx:id="LayerNum" prefWidth="150.0" />
            <Label text="Number of  design:" />
            <ComboBox fx:id="DesignNum" prefWidth="150.0" />
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
    </children>
</AnchorPane>
