<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cm.controllers.ImportTabController">
    <children>
        <ButtonBar layoutX="550" layoutY="450" prefHeight="40.0" prefWidth="200.0">
            <buttons>
                <Button mnemonicParsing="false" onAction="#saveButtonAction" text="Save" />
                <Button mnemonicParsing="false" text="Close" />
            </buttons>
        </ButtonBar>
      <VBox fx:id="vBox1" layoutX="60.0" layoutY="93.0" prefHeight="204.0" prefWidth="240.0" spacing="10.0">
         <children>
            <Label text="Design 1">
               <padding>
                  <Insets left="50.0" />
               </padding>
            </Label>
            <Label text="Design type:" />
            <ComboBox fx:id="designType1ComboBox" onAction="#updatePavementTypeComboBox" prefWidth="150.0" />
            <Label fx:id="label1" text="Pavement type:" />
            <ComboBox fx:id="pavementType1ComboBox" prefWidth="150.0" />
            <Label text="Number of layers to be analyized:" />
            <ComboBox fx:id="numberOfLayers1ComboBox" prefWidth="150.0" />
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
      <Label layoutX="209.0" layoutY="50.0" prefHeight="15.0" prefWidth="240.0" text="Select number of designs" />
      <ComboBox fx:id="numberOfDesignsComboBox" layoutX="374.0" layoutY="46.0" onAction="#initialize" prefWidth="150.0" />
      <VBox fx:id="vBox2" layoutX="273.0" layoutY="93.0" prefHeight="204.0" prefWidth="240.0" spacing="10.0">
         <children>
            <Label text="Design 2">
               <padding>
                  <Insets left="50.0" />
               </padding>
            </Label>
            <Label text="Design type:" />
            <ComboBox fx:id="designType2ComboBox" onAction="#updatePavementTypeComboBox" prefWidth="150.0" />
            <Label fx:id="label2" text="Pavement type:" />
            <ComboBox fx:id="pavementType2ComboBox" prefWidth="150.0" />
            <Label text="Number of layers to be analyized:" />
            <ComboBox fx:id="numberOfLayers2ComboBox" prefWidth="150.0" />
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
      <VBox fx:id="vBox3" layoutX="491.0" layoutY="93.0" prefHeight="204.0" prefWidth="240.0" spacing="10.0">
         <children>
            <Label text="Design 3">
               <padding>
                  <Insets left="50.0" />
               </padding>
            </Label>
            <Label text="Design type:" />
            <ComboBox fx:id="designType3ComboBox" onAction="#updatePavementTypeComboBox" prefWidth="150.0" />
            <Label fx:id="label3" text="Pavement type:" />
            <ComboBox fx:id="pavementType3ComboBox" prefWidth="150.0" />
            <Label text="Number of layers to be analyized:" />
            <ComboBox fx:id="numberOfLayers3ComboBox" prefWidth="150.0" />
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
    </children>
</AnchorPane>
